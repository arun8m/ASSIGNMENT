
n= int(input("Enter "))
def factorial(n):
    if n < 2:
        return 1
    else:
        return n * (factorial(n-1))


result = factorial(n)
print ("Factorial of ", n, "is:", result)


import math

n= int(input("Enter a number:"))
print("Square root:", math.sqrt(n))
print("Logarithm:", math.log(n))
print("Sine:", math.sin(n))
